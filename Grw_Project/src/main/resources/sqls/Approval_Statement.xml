<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.rgb.grw.dao.TemplatePreviewDaoImpl">
  	<select id="selectTemplate" resultType="TemplatePreviewDto">
  		SELECT TEMP_ID, TEMP_TITLE, TEMP_CONTENT, TEMP_REGDATE, TEMP_EDITDATE
			FROM TEMPLATE t
  	</select>
  	
  	<select id="jstreeDep" resultType="DeptDto">
	  		SELECT *
				FROM DEP d
  	</select>
  	
  	<select id="jstreeEmp" resultType="EmpDto">
		SELECT *
			FROM EMP e
  	</select>
</mapper>
